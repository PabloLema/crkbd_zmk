        keymap {
                compatible = "zmk,keymap";

                default_layer {
// -----------------------------------------------------------------------------------------
// |  TAB |  Q  |  W  |  E  |  R  |  T  |   |  Y  |  U   |  I  |  O  |  P  | BKSP |
// | CTRL |  A  |  S  |  D  |  F  |  G  |   |  H  |  J   |  K  |  L  |  ;  |  '   |
// | SHFT |  Z  |  X  |  C  |  V  |  B  |   |  N  |  M   |  ,  |  .  |  /  | ESC  |
//                    | GUI | LWR | SPC |   | ENT | RSE  | ALT |
                        bindings = <
   &td3      &td0  &td1  &td2  &kp P &kp Y   &kp F &kp G  &kp C     &kp R   &kp L    &kp BSPC
   &mt LSHIFT TAB      &kp A &kp O &kp E &kp U &kp I   &kp D &kp H  &kp T     &kp N   &kp S    &td4
   &kp LGUI  &td5  &kp Q &kp J &kp K &kp X   &kp B &kp M  &kp W     &kp V   &kp Z    &kp RGUI
                   &kp LCTRL &mo 1 &kp SPACE   &kp RET &mo 2 &kp RALT
                        >;
                };
                lower_layer {
// -----------------------------------------------------------------------------------------
// |  TAB |  1  |  2  |  3  |  4  |  5  |   |  6  |  7  |  8  |  9  |  0  | BKSP |
// | BTCLR| BT1 | BT2 | BT3 | BT4 | BT5 |   | LFT | DWN |  UP | RGT |     |      |
// | SHFT |     |     |     |     |     |   |     |     |     |     |     |      |
//                    | GUI |     | SPC |   | ENT |     | ALT |
                        bindings = <
   &td3    &kp N1       &kp N2       &kp N3       &kp N4       &kp N5         &kp N6   &kp N7   &kp N8 &kp N9    &kp N0 &kp BSPC
   &mt LSHIFT TAB       &trans   &bt BT_SEL 0 &bt BT_SEL 1 &bt BT_SEL 2 &bt BT_SEL 3   &kp RG(GRAVE) &kp LEFT &kp UP &kp DOWN &kp RIGHT &kp RG(SLASH)
   &kp LGUI  &trans       &trans       &kp LG(C)       &kp LG(V)       &trans    &trans   &kp LA(LEFT)   &kp LG(LEFT) &kp LG(RIGHT)    &kp LA(RIGHT) &kp RGUI
                          	        &kp LCTRL     &trans       &kp SPACE      &kp RET  &mo 3   &trans
                        >;
                };

                raise_layer {
// -----------------------------------------------------------------------------------------
// |  TAB |  !  |  @  |  #  |  $  |  %  |   |  ^  |  &  |  *  |  (  |  )  | BKSP |
// | CTRL |     |     |     |     |     |   |  -  |  =  |  [  |  ]  |  \  |  `   |
// | SHFT |     |     |     |     |     |   |  _  |  +  |  {  |  }  | "|" |  ~   |
//                    | GUI |     | SPC |   | ENT |     | ALT |
                        bindings = <
   &td3  TAB  &kp EXCL &kp AT &kp HASH &kp DLLR &kp PRCNT   &kp CARET &kp AMPS  &kp KP_MULTIPLY &td6 &td7 &kp BSPC
   &mt LSHIFT TAB      &trans   &td10 &td11   &td12   &trans      &trans &td8 &td9        &trans &trans &bt BT_CLR
   &kp LGUI  &trans   &trans &trans   &kp F12   &trans      &trans &trans  &trans        &trans &trans &kp RGUI
                    	     &kp LGUI &trans   &kp SPACE   &kp RET   &trans    &kp RALT
                        >;
                };
//                mix {
//                        bindings = <
//   &trans  &trans &trans &trans &trans &trans   &trans &trans &trans &trans &trans &trans
//   &trans  &trans &trans &kp PG_DN &kp PG_UP &trans   &trans &kp C_PP &kp C_PREV &kp C_NEXT &trans &trans
//   &trans  &trans &trans &trans &trans &trans   &trans &kp C_VOL_UP &kp C_VOL_DN &trans &trans &trans
//                    	   &trans &trans &trans   &trans &trans &trans
//                        >;
//                };
        };
